    <script>
        function matchPassword(confirmPw) {
            var newPw = $('#password').val();
            if (newPw != confirmPw) {
                $('#pwErrMsg').text("Password & Confirm Password did not match");
                $('#RegisterForm').attr("disabled", true);
            }
            else {
                $('#pwErrMsg').text("");
                $('#RegisterForm').attr("disabled", false);
            }
        }

        function validateEmail(email) {
            if (email != "") {
                $.ajax({
                    type: 'POST',
                    url: "../Ajax/ValidateEmail",
                    dataType: "json",
                    data: {
                        email: email, id: -1
                    },
                    success: function (response) {
                        if (response == true) {
                            $('#emailErrMsg').text("");
                            $('#RegisterForm').attr('disabled', false);
                        }
                        else {
                            $('#emailErrMsg').text("Email already exsist");
                            $('#RegisterForm').attr('disabled', true);
                        }
                    },
                    error: function () {
                        $('#emailErrMsg').text("");
                        $('#RegisterForm').attr('disabled', false);
                        alert("Ajax failed");
                    }
                });
            }

        }

        function validateUsername(username) {
            if (username != "") {
                $.ajax({
                    type: 'POST',
                    url: "../Ajax/ValidateUsername",
                    dataType: "json",
                    data: {
                        username: username, id: -1
                    },
                    success: function (response) {
                        if (response == true) {
                            $('#nameErrMsg').text("");
                            $('#RegisterForm').attr('disabled', false);
                        }
                        else {
                            $('#nameErrMsg').text("Username already exsist");
                            $('#RegisterForm').attr('disabled', true);
                        }
                    },
                    error: function () {
                        $('#nameErrMsg').text("");
                        $('#RegisterForm').attr('disabled', false);
                        alert("Ajax failed");
                    }
                });
            }

        }
    </script>
